   ],
   "source": [
    "def fib(n):\n",
    "    f,s = 0,1\n",
    "    if n == 0:\n",
    "        yield f\n",
    "    else:\n",
    "        for _ in range(n):\n",
    "            yield f\n",
    "            f, s = s, f+s \n",
    "\n",
    "[n for n in fib(10)]\n",
    "    "
   ]
  }
